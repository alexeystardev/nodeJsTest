<section class="page-section bg-light" id="portfolio">
	<div class="container">
		<div class="text-center">
			<h2 class="section-heading text-uppercase mt-2">Movies</h2>
			<h3 class="section-subheading text-muted">Our Collection of the Movies</h3>
		</div>
		<div class="row text-center">
			<% movies.Search.forEach(element => { %>
			<%- include('../main/movies/moviesTemplate.ejs',{img:element.Poster, title:element.Title, year:element.Year}) %>
			<% }); %>
		</div>
	</div>
</section>